[
    {?any}*.
    {SELF ?C0:String ?P0:String !"${T0}"}.
    {?any}*.
    {SELF ?C1:String ?P1:String !"${T1}"}.
    {?any}*.
    {SELF ?C2:String ?P2:String !"${T2}"  where
        (inter (string2natset (P1), inter(string2natset (P0),string2natset (P2))) <> empty)
    }
]print(inter ( inter (string2natset (C0),string2natset (C1)), string2natset (C2)),"$",inter (string2natset (P1), inter(string2natset (P0),string2natset (P2))) ,"\n")